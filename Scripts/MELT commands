# MELT GroupAnalysis

MELT.jar GroupAnalysis -discoverydir /labs/dflev/hzudohna/1000Genomes/L1_simulation_MELT/ -h /labs/dflev/hzudohna/RefSeqData/hg19.fa -t /labs/dflev/hzudohna/MELTv2.1.5/me_refs/1KGP_Hg19/LINE1_MELT.zip -n /labs/dflev/hzudohna/MELTv2.1.5/add_bed_files/1KGP_Hg19/hg19.genes.bed -w /labs/dflev/hzudohna/1000Genomes/L1_simulation_MELT/


# MELT genotype

MELT.jar Genotype -bamfile /labs/dflev/hzudohna/1000Genomes/L1_simulation_MELT//hg19_HG00109_sorted.bam -h /labs/dflev/hzudohna/RefSeqData/hg19.fa -t /labs/dflev/hzudohna/MELTv2.1.5/me_refs/1KGP_Hg19/LINE1_MELT.zip -w /labs/dflev/hzudohna/1000Genomes/L1_simulation_MELT/ -p /labs/dflev/hzudohna/1000Genomes/L1_simulation_MELT/

MELT.jar Genotype -bamfile /labs/dflev/hzudohna/1000Genomes/L1_simulation_MELT//hg19_HG00120_sorted.bam -h /labs/dflev/hzudohna/RefSeqData/hg19.fa -t /labs/dflev/hzudohna/MELTv2.1.5/me_refs/1KGP_Hg19/LINE1_MELT.zip -w /labs/dflev/hzudohna/1000Genomes/L1_simulation_MELT/ -p /labs/dflev/hzudohna/1000Genomes/L1_simulation_MELT/

